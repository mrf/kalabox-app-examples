{
  "appName": "drupal7",
  "appPlugins": [
    "kalabox-plugin-pressflow7-env",
    "kalabox-plugin-git",
    "kalabox-plugin-drush"
  ],
  "pluginConf": {
    "kalabox-plugin-drush": {
      "drush-version": "drush6"
    }
  },
  "appComponents": {
    "data": {
      "image": {
        "name": "kalabox/data:stable"
      }
    },
    "db": {
      "image": {
        "name": "kalabox/mariadb:stable"
      }
    },
    "php": {
      "image": {
        "name": "kalabox/php-fpm:stable"
      }
    },
    "web": {
      "image": {
        "name": "kalabox/nginx:stable"
      },
      "proxy": [
        {
          "port": "80/tcp",
          "default": true
        }
      ]
    }
  }
}
